# IF / ELSE EXAMPLE

# hungry = True
hungry = False

if hungry:
    print("Feed me")
else:
    print("I'm not hungry")

# IF / ELIF / ELSE EXAMPLE

loc = "Bank"

if loc == "Auto Shop":
    print("Cars are cool!")
elif loc == "Bank":
    print("Money is cool!")
elif loc == "Store":
    print("Welcome to the store!")
else:
    print("I don't know")
