class Dog:

    # constructor
    def __init__(self, breed, name, spots):
        # Attributes
        self.breed = breed
        self.name = name
        self.spots = spots


my_dog = Dog(breed='Lab', name='Sammy', spots=False)

print(type(my_dog))
print(my_dog.breed)
print(my_dog.name)
print(my_dog.spots)
